def add(num1, num2):
    return num1 + num2
print(add(4,2))

def sub(num1, num2):
    return num1 - num2
print(sub(4,2))

def mul(num1, num2):
    return num1 * num2
print(mul(4,2))

def div(num1, num2):
    if num2 == 0:
        return "Cannot divide by zero"
    return num1 / num2
print(div(4,2))
